#include<stdio.h>
int main()
{
	int f,l,mid,n,i,key,a[100],flag=0;
	printf("enter the num of ele:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the element to search:");
	scanf("%d",&key);
	f=0;
	l=n-1;
	while(f<=l)
	{
		mid=(f+l)/2;
		if(a[mid]==key)
		{
			flag=1;
			break;	
		}
		else if (a[mid]<key)
		{
			f=mid+1;
		}
		else if(a[mid]>key)
		{
			l=mid-1;
		}
	}
	if(flag==1)
	{
		printf("%d is found at %d position",key,mid);
	}
	else
	printf("not found");
}
